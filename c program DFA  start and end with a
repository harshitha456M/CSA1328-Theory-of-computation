#include<stdio.h>
#include<string.h>
int main()
{
	char string[100];
	printf("enter string:");
	scanf("%s",string);
	if(string[0]=='a'&&string[strlen(string)-1]=='a')
	{
		for(int i=0;i<strlen(string);i++)
		{
			if(string[0]=='0'||string[1]=='1')
			{
				printf("invalid\n");
			}
}
	printf("accepted");
    }
		else
		{
		printf("not accepted");	
		}
}
